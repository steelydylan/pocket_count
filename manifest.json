{
  "name": "PocketCount",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {"48": "icon.png"},
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "tabs"
  ]
}